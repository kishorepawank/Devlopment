pipeline {
    agent any
    parameters {
        string defaultValue: 'Dev', description: 'my env', name: 'Env', trim: false
    }
    stages {
        stage('Build') { 
            steps {
                echo "this is Build"
            }
        }
        stage('Test') { 
            steps {
                echo "this is Test"
            }
        }
        stage('Deploy') { 
            steps {
               echo "this is Deploy"
            }
        }
    }
}
